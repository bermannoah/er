<html>
<head>
  <meta charset='utf-8'>
  <title><%= filename %></title>
  <link href="stylesheet.css" rel="stylesheet" type="text/css">
</head>
<body>
  <div class="header">Queue Output printed on <%= Time.now.strftime("%d/%m/%Y at %H:%M") %>.</div>

  <table class="table">
    <tr class="table_head"> 
      <th class="table_head">Last Name</th> <th class="table_head">First Name</th>
      <th class="table_head">Email</th> <th class="table_head">Zipcode</th>
      <th class="table_head">City</th> <th class="table_head">State</th>
      <th class="table_head">Address</th> <th class="table_head">Phone</th>
    </tr>
    
  
    <% @queue_results.each do |attendee| %>
      <tr class="table">
        <td class="table"><%= "#{attendee.last_name}" %></td> 
        <td class="table"><%= "#{attendee.first_name}" %></td> 
        <td class="table"><%= "#{attendee.email}" %></td> 
        <td class="table"><%= "#{attendee.zipcode}" %></td> 
        <td class="table"><%= "#{attendee.city}" %></td> 
        <td class="table"><%= "#{attendee.state}" %></td> 
        <td class="table"><%= "#{attendee.street_address}" %></td>
        <td class="table"><%= "#{attendee.phone_number}" %></td>
      </tr>
    <% end %>

  </table>

</body>
</html>
